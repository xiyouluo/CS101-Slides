{
    "version": 1,
    "include": [
        "/*"
    ],
    "exclude": [
        "/.*\\.(js|css|png|jpg|jpeg|gif|svg|woff|woff2|ico|json|md|txt|mp4|webm|pdf)$"
    ],
    "rules": [
        {
            "source": "^/([^/.]+)/?$",
            "destination": "/$1/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}